    <!-- Contact-->
    <section class="page-section" id="contato">
        <div class="container">
            <div class="text-center">
                <h2 class="section-heading text-uppercase">Contato</h2>
                <h3 class="section-subheading text-muted">
                    Estaremos de prontidão para te antender!
                </h3>
            </div>
            @using (Html.BeginForm("ValidarDados", "Home", FormMethod.Post))
            {
                <div class="row align-items-stretch mb-5">
                    <div class="col-md-6">
                        <div class="form-group">
                            <input class="form-control"
                                   name="nome"
                                   type="text"
                                   placeholder="Seu Nome *"
                                   required="required"
                                   data-validation-required-message="Porfavor, entre com um nome." />
                            <p class="help-block text-danger"></p>
                        </div>
                        <div class="form-group">
                            <input class="form-control"
                                   name="email"
                                   type="text"
                                   placeholder="Seu E-Mail *"
                                   required="required"
                                   data-validation-required-message="Porfavor, entre com um nome." />
                            <p class="help-block text-danger"></p>
                        </div>
                        <div class="form-group mb-md-0">
                            <input class="form-control"
                                   name="telefone"
                                   type="tel"
                                   placeholder="Seu Telefone/ WhatsApp *"
                                   required="required"
                                   data-validation-required-message="Porfavor, entre com um numero de telefone." />
                            <p class="help-block text-danger"></p>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="form-group form-group-textarea mb-md-0">
                            <textarea class="form-control"
                                      name="mensagem"
                                      placeholder="Sua Mensagem *"
                                      required="required"
                                      data-validation-required-message="Porfavor, entre com uma mensagem."></textarea>
                            <p class="help-block text-danger"></p>
                        </div>
                    </div>
                </div>
                <div class="text-center">
                    <div id="success"></div>
                    <button class="btn btn-primary btn-xl text-uppercase"
                            id="sendMessageButton"
                            type="submit">
                        Enviar Mensagem
                    </button>
                </div>
            }
        </div>
    </section>
